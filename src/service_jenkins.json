{
    "service": {
        "name": "build",
        "id": "jenkins",
        "tags": [
            "inactive",
            "urlprefix-/builds"
        ],
        "port": 8080,
        "enableTagOverride": false,
        "checks": [
            {
                "id": "jenkins_ping",
                "name": "Jenkins HTTP ping",
                "http": "http://localhost:8080/builds/metrics/${GuidHealthPing}/ping",
                "interval": "15s"
            },
            {
                "id": "jenkins_health",
                "name": "Jenkins HTTP health check",
                "http": "http://localhost:8080/builds/metrics/${GuidHealthCheck}/healthcheck",
                "interval": "60s"
            }
        ]
    }
}
